@model RichardMCano.Website.Models.Education.EducationViewModel

<!DOCTYPE html>
<html>
<head>
    <title>@Model.Title</title>
    <meta name="description" content="Richard M. Cano Resume - Education" />

    @Html.Partial("_HeadScripts")
    <script src="~/Scripts/Views/Education/Index.js"></script>

    <!-- CSS -->
    <link rel="stylesheet" type="text/css" media="screen and (min-width: 1001px)" href="~/Content/CSS/Education/Education.css">
    <link rel="stylesheet" type="text/css" media="screen and (min-width 826px) and (max-width 1000px)" href="~/Content/CSS/Education/Education_1000px.css">
    <link rel="stylesheet" type="text/css" media="screen and (min-width 761px) and (max-width 825px)" href="~/Content/CSS/Education/Education_825px.css">
    <link rel="stylesheet" type="text/css" media="screen and (min-width 626px) and (max-width 760x)" href="~/Content/CSS/Education/Education_760px.css">
    <link rel="stylesheet" type="text/css" media="screen and (min-width 426px) and (max-width 625x)" href="~/Content/CSS/Education/Education_625px.css">
    <link rel="stylesheet" type="text/css" media="screen and (max-width 425x)" href="~/Content/CSS/Education/Education_425px.css">
</head>

<body>

    <div class="page">
        <header>
            <a class="logo" title="Richard M. Cano Resume" href="http://localhost:62403/"><span>Richard M. Cano Resume</span></a>
            <div class="">
                <h1></h1>
            </div>
        </header>
        <section class="main">
            <div class="listItems educationList">
                @foreach (var education in Model.Educations)
                {
                    <div class="btn btnEducationList" id="@education.EducationGUID">
                        @education.SchoolName
                        <div id="degreeName">
                            @education.DegreeName
                        </div>
                    </div>
                }
            </div>
            <aside>
                <div>

                </div>
            </aside>
        </section>
        <section class="intro">
            @Html.Partial("_sctIntro")
        </section>
        @Html.Partial("_Navigation")
        <footer>
            &copy; 2017. Richard M Cano Resume
        </footer>
    </div>

</body>
</html>